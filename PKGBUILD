pkgname=youtomp3
pkgver=0.1
pkgrel=1
pkgdesc="Frontend to youtube-mp3"
arch=('i686' 'x86_64')
url="http://enzo.oldvolks.net"
license=('GPL')
depends=('perl' 'youtube-mp3')
source=(http://enzo.oldvolks.net/youtomp3.tar)

build() {
  install -Dm755 youtomp3 "$pkgdir/usr/bin/youtomp3"
}
md5sums=('37f43f034940cc7ce596eda8afb8be10')

